  "compilerOptions": {
    "incremental": true,
    "target": "ES6",
    "module": "ES6",
    "lib": ["ES6", "DOM"],
    "allowJs": true,
    "outDir": "./out",
    "strict": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}
